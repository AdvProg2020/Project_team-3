<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Menus.MenuController.SellerMenuController.SellerManageProductsMenu">

    <MenuBar layoutY="1.0" prefHeight="25.0" prefWidth="1280.0">
        <menus>
            <Menu mnemonicParsing="false" text="option">
                <MenuItem mnemonicParsing="false" onAction="#addProduct" text="Add Product" />
                <MenuItem mnemonicParsing="false" onAction="#back" text="back" />
                <MenuItem mnemonicParsing="false" onAction="#logout" text="logout" />
            </Menu>
        </menus>
    </MenuBar>
    <ListView fx:id="listView" layoutX="389.0" layoutY="131.0" onMouseClicked="#requestSelect" prefHeight="459.0" prefWidth="502.0" />
   <Button layoutX="925.0" layoutY="131.0" mnemonicParsing="false" onAction="#addProduct" prefHeight="25.0" prefWidth="101.0" text="Add Product" />
   <Label layoutX="389.0" layoutY="84.0" prefHeight="33.0" prefWidth="101.0" text="Your Items:" />
   <TextField fx:id="itemTextField" layoutX="389.0" layoutY="615.0" prefHeight="25.0" prefWidth="169.0" promptText="Enter an Item ID" />
   <Button layoutX="569.0" layoutY="615.0" mnemonicParsing="false" onAction="#manageProduct" text="Manage" />
   <Label fx:id="manageError" layoutX="640.0" layoutY="619.0" prefHeight="17.0" prefWidth="253.0" />

</AnchorPane>
